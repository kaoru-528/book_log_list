<li>
    <%=image_tag user.image, class:'icon',size:60 %>
    <% check_anonymous(user) %>
    <%= link_to user.name, user %>
    <% if current_user.admin? && !current_user?(user) %>
        | <%= link_to "delete", user, data: { "turbo-method": :delete,
                                                turbo_confirm: "You sure?" }%>
    <% end %>
</li>